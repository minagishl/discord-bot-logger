{
  "name": "discord-bot-logger",
  "version": "0.1.0",
  "description": "It sends all logs within the specified Discord server to a specific channel.",
  "license": "MIT",
  "author": "minagishl",
  "type": "commonjs",
  "scripts": {
    "build": "yarn rm && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "tsx src/index.ts",
    "esbuild": "yarn rm && node scripts/build.js",
    "check": "biome check ./src",
    "check:write": "biome check --write ./src",
    "format": "biome format ./src",
    "format:write": "biome format --write ./src",
    "lint": "biome lint ./src",
    "register": "tsx src/register.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "rm": "node scripts/rm.js"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint:fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@types/express": "^5.0.5",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/node": "^24.10.1",
    "esbuild": "^0.27.0",
    "lint-staged": "^16.2.6",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild"
    ]
  }
}
